{
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "version": "0.56.3",
  "default_locale": "en",
  "author": "Kesantielu Dasefern",
  "homepage_url": "https://kesantielu.github.io/oldvk",
  "icons": {
    "48": "oldvk-48.png",
    "64": "oldvk-64.png",
    "128": "oldvk-128.png"
  },
  "browser_action": {
    "default_icon": "oldvk-48.png",
    "default_popup": "popup.html",
    "browser_style": false
  },
  "permissions": [
    "*://*.vk.com/*",
    "*://vk.com/*",
    "tabs",
    "storage"
  ],
  "background": {
    "scripts": [
      "events.js"
    ]
  },
  "options_ui" : {
    "page": "options.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://vk.com/*"
      ],
      "js": [
        "lib/lib.js",
        "content/mainStart.js"
      ],
      "run_at": "document_start",
      "exclude_matches": [
        "*://vk.com/notifier.php*",
        "*://vk.com/al_*.php*",
        "*://vk.com/dev/*",
        "*://vk.com/apps?act=manage*",
        "*://vk.com/upload_fails.php*",
        "*://vk.com/ads_rotate.php*",
        "*://vk.com/share.php*",
        "*://vk.com/adscreate*",
        "*://vk.com/wkview.php*",
        "*://vk.com/bugs",
        "*://vk.com/bugs?*",
        "*://vk.com/login.php*",
        "*://vk.com/blog",
        "*://vk.com/blog?*",
        "*://vk.com/q_frame.php*",
        "*://vk.com/login.php*",
        "*://vk.com/about*",
        "*://vk.com/jobs*",
        "*://vk.com/*.svg*",
        "*://vk.com/widget_*.php*"
      ],
      "all_frames": true
    },
    {
      "matches": [
        "*://vk.com/*"
      ],
      "js": [
        "content/mainEnd.js"
      ],
      "exclude_matches": [
        "*://vk.com/notifier.php*",
        "*://vk.com/al_*.php*",
        "*://vk.com/dev/*",
        "*://vk.com/apps?act=manage*",
        "*://vk.com/upload_fails.php*",
        "*://vk.com/ads_rotate.php*",
        "*://vk.com/share.php*",
        "*://vk.com/adscreate*",
        "*://vk.com/wkview.php*",
        "*://vk.com/bugs",
        "*://vk.com/bugs?*",
        "*://vk.com/login.php*",
        "*://vk.com/blog",
        "*://vk.com/blog?*",
        "*://vk.com/q_frame.php*",
        "*://vk.com/login.php*",
        "*://vk.com/about*",
        "*://vk.com/jobs*",
        "*://vk.com/*.svg*",
        "*://vk.com/widget_*.php*"
      ],
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    "content/*.css",
    "content/injectStart.js",
    "content/injectEnd.js"
  ],
  "applications": {
    "gecko": {
      "id": "{6acba1db-bca7-4dc3-b20e-3230c4f5a54e}",
      "strict_min_version": "48.0",
      "update_url": "https://kesantielu.github.io/oldvk/updates.json"
    }
  }
}